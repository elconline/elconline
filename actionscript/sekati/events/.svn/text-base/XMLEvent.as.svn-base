/** * sekati.events.XMLEvent * @version 1.0.0 * @author jason m horwitz | sekati.com * Copyright (C) 2008  jason m horwitz, Sekat LLC. All Rights Reserved. * Released under the MIT License: http://www.opensource.org/licenses/mit-license.php */package sekati.events {	import flash.events.Event;	/**	 * XMLEvent	 * @see sekati.load.XMLLoader	 */	public class XMLEvent extends Event {		/**		 * @eventType onReady fires when the xml initializes.		 */		public static const ON_READY : String = "onReady";				protected var _data : XML;		/**		 * XMLEvent Constructor		 */		public function XMLEvent(type : String, xmlData : XML, bubbles : Boolean = false, cancelable : Boolean = false) {			super( type, bubbles, cancelable );			data = xmlData;					}		/**		 * @inheritDoc		 */		override public function clone() : Event {				return new XMLEvent( type, _data );		}		/**		 * The <code>XMLEvent</code>'s XML content data.		 * @return XML		 */		public function get data() : XML {			return _data;		}		/**		 * @private		 */		public function set data(xmlData : XML) : void {			_data = xmlData;		}						}}